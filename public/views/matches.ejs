<html>

<head>
 <%- include partials/head.ejs %>
</head><!-- /head -->

<body>

 <header>
 <%- include partials/header.ejs %>
 </header>

<div class="logosmall"></div>

<h2>Here are your ideal housemates <%- user.username %></h2>
<h3>Check back every day for new matches</h3>

<table class="table table-dark">

  <thead class="row">
    <tr>
      <td>Username</td>
      <td>About</td>
    </tr>
  </thead>

<tbody>
  <tr>
    <% for (let i= 0; i < matches.length; i++) { %>
    <td>
      <%- matches[i].user.username %>
    </td>
    <td>
      <%- matches[i].user.about %>
    </td>
  </tr>

  <% } %>
</tbody>
</table>


<footer>
 <%- include partials/footer.ejs %>
</footer>

</body>

</html>
